# LS증권 API 설정
LS_APP_KEY=your_app_key_here
LS_APP_SECRET=your_app_secret_here
LS_ACCOUNT_NO=your_account_number_here

# LS증권 API URL (모의투자/실전투자)
# 모의투자: https://openapi.ls-sec.co.kr:29443
# 실전투자: https://openapi.ls-sec.co.kr:9443
LS_API_URL=https://openapi.ls-sec.co.kr:29443

# Supabase 설정
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Telegram 알림 설정
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# 환경 설정
ENV=development  # development | production
LOG_LEVEL=INFO   # DEBUG | INFO | WARNING | ERROR
